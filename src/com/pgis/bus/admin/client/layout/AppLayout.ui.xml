<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:gwt="urn:import:com.google.gwt.user.client.ui">
	
	<gwt:DockLayoutPanel unit="PX">
		<gwt:north size="29.0">
			<gwt:HTMLPanel styleName="b-app-header">
				<gwt:Label height="17px" horizontalAlignment="ALIGN_LEFT" text="TransCity Web - Administration panel" width="448px"/>
				<gwt:Label text="New Label" width="103px" horizontalAlignment="ALIGN_LEFT" ui:field="label_user"/>
				
				
			</gwt:HTMLPanel>
		</gwt:north>	
		
		<gwt:west size="200">
			<gwt:HTMLPanel styleName="b-app-navigator">
				<a id="stations-view" href="#stations:">Stations</a>
				<a id="routes-view"   href="#routes:">Routes</a>
				<gwt:Button ui:field="button">Exit</gwt:Button>
			</gwt:HTMLPanel>
		</gwt:west>
		
		<gwt:center>
			<gwt:SimplePanel styleName="b-app-content" ui:field="appContent" />
		</gwt:center>
	</gwt:DockLayoutPanel>
	
</ui:UiBinder>
 